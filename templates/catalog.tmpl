<:wrap base.tmpl:><:embed start:>
<!-- START ID: <:article id:> - TEMP: <:article template:> --><:if Embedded:><:admin article_mini:><:or Embedded:><:admin:><:eif Embedded:>
<div id="article_<:article id:>" class="article shop catalog"><:if Embedded:>
<h1 id="title_<:article id:>" class="embedtitle"><:article title:></h1><:or Embedded:>
<h1 id="title_<:article id:>" class="pagetitle"><:title:></h1><:eif Embedded:><:if Or [article body] [ifImages]:>
<div id="body_<:article id:>" class="body">
<:body:>
</div><:or Or:><:eif Or:><:switch:><:case Allprods:><:include include/sortallkids.tmpl:><:if UnderThreshold allkids:><:iterator begin allprods:>
<:movestepkid:><:embed allprod:><:iterator separator allprods:><:iterator end allprods:><:or UnderThreshold:>
<form id="add_multiple_form_<:article id:>" action="/cgi-bin/shop.pl" method="post">
<input type="hidden" name="r" value="<:url top:>" /><:if Cfg shop catalog_list:>
<div class="items">
  <ul><:iterator begin allprods:>
    <li<:ifAllprod thumbImage:> class="thumbnail"<:or:><:eif:>>
      <div class="col1"><:ifAllprod thumbImage:>
        <a href="<:url allprod:>" class="thumbnail"><:thumbnail allprod:></a><:or:><:eif:>
        <a href="<:url allprod:>" class="title"><:allprod title:></a><:if And [allprod description] [cfg shop product_description 1]:>
        <div class="i_description"><:allprod description:></div><:or And:><:eif And:>
      </div>
      <div class="col2"><input type="text" name="qty<:allprod id:>" id="qty<:allprod id:>" size="2" tabindex="<:allprod_number:>" /></div>
      <div class="col3"><:ifAllprod retailPrice:><:cfg shop currency "$":><:money allprod retailPrice:><:or:>NA<:eif:></div>
      <div class="col4">
        <input type="image" name="a_addsingle<:allprod id:>" id="a_addsingle<:allprod id:>" src="/images/basic/store/buttons/add.gif" title="Add this item" class="shop_addsingle" /><:if Admin:>
        <span class="arrows"><:include admin/adminmenu/allprod_mini.tmpl:><:moveallprod:></span><:or Admin:><:eif Admin:>
      </div>
    </li><:iterator end allprods:>
  </ul>
</div><:or Cfg:>
<table class="items">
  <caption>Select Order Items</caption>
  <thead>
    <tr>
      <th class="col1">Item:</th>
      <th class="col2">Qty:</th>
      <th class="col3">Price:</th>
      <th class="col4">&nbsp;</th>
    </tr>
  </thead>
  <tbody><:iterator begin allprods:>
    <tr class="item">
      <td class="col1"><:ifAllprod thumbImage:>
        <a href="<:url allprod:>" class="thumbnail"><:thumbnail allprod:></a><:or:><:eif:>
        <a href="<:url allprod:>" class="title"><:allprod title:></a><:if And [allprod description] [cfg shop product_description 1]:>
        <div class="i_description"><:allprod description:></div><:or And:><:eif And:>
      </td>
      <td class="col2"><input type="text" id="qty<:allprod id:>" name="qty<:allprod id:>" size="2" /></td>
      <td class="col3"><:ifAllprod retailPrice:><:cfg shop currency "$":><:money allprod retailPrice:><:or:>NA<:eif:></td>
      <td class="col4"><input type="image" name="a_addsingle<:allprod id:>" id="a_addsingle<:allprod id:>" src="/images/basic/store/buttons/add.gif" title="Add this item" class="shop_addsingle" /><:if Admin:> <:include admin/adminmenu/allprod_mini.tmpl:><:moveallprod:><:or Admin:><:eif Admin:></td>
    </tr><:iterator end allprods:>
  </tbody>
</table><:eif Cfg:>
<p class="buttons">
  <input type="submit" id="addmultiple<:article id:>" name="a_addmultiple" value="Add items to cart" accesskey="a" title="AccessKey = a" tabindex="10000" class="shop_addmultiple" />
  <input type="submit" name="cart" value="View cart" accesskey="v" title="AccessKey = v" tabindex="11000" />
</p>
</form><:eif UnderThreshold:><:endswitch:><:switch:><:case Allcats:>
<div class="children">
  <ul><:iterator begin allcats:>
    <li<:ifAllcat thumbImage:> class="thumbnail"<:or:><:eif:>><:ifAllcat thumbImage:>
      <a href="<:url allcat:>" class="thumbnail"><:thumbnail allcat:></a><:or:><:eif:>
      <a href="<:url allcat:>" class="title"><:allcat title:></a><:if Admin:>
      <span class="arrows"><:include admin/adminmenu/allcat_mini.tmpl:><:moveallcat:></span><:or Admin:><:eif Admin:><:switch:><:case Allcat summary:>
      <span class="summary"><:allcat summary:></span><:case Allcat summaryLength:>
      <span class="summary"><:summary allcat:></span><:endswitch:>
    </li><:iterator end allcats:>
  </ul>
</div><:endswitch:><:if Or [ifEmbedded] [cfg basic hide_stepparent_list]:><:or Or:><:if Stepparents:>
<div class="relatedsections">
  <p><strong>Related sections:</strong></p>
  <ul><:iterator begin stepparents:>
    <li><a href="<:url stepparent:>"><:stepparent title:></a></li><:iterator end stepparents:>
  </ul>
</div><:or Stepparents:><:eif Stepparents:><:eif Or:><:if Files:><:if Or [cfg basic hide_file_list] [ifMatch [article flags] "H"]:><:or Or:>
<:include include/files.tmpl:><:eif Or:><:or Files:><:eif Files:>
</div>
<!-- END ID: <:article id:> - TEMP: <:article template:> -->
<:embed end:>