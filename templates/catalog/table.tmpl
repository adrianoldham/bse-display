<:wrap base.tmpl:><:embed start:>
<!-- START ID: <:article id:> - TEMP: <:article template:> --><:if Embedded:><:admin article_mini:><:or Embedded:><:admin:><:eif Embedded:>
<div id="article_<:article id:>" class="article"><:if Embedded:>
<h1 id="title_<:article id:>" class="embedtitle"><:article title:></h1><:or Embedded:>
<h1 id="title_<:article id:>" class="pagetitle"><:title:></h1><:eif Embedded:><:if Or [article body] [ifImages]:>
<div id="body_<:article id:>" class="body">
<:body:>
</div><:or Or:><:eif Or:><:switch:><:case And [ifAllprods] [ifUnderThreshold allprods]:><:include include/sortallkids.tmpl:><:iterator begin allprods:>
<:moveallprod:><:embed allprod:><:iterator end allprods:><:case Allkids:><:include include/sortallkids.tmpl:>
<form id="add_multiple_form_<:article id:>" action="/cgi-bin/shop.pl" method="post">
<input type="hidden" name="r" value="<:url top:>" />
<div class="children">
  <table class="products">
    <tr><:iterator begin allprods:><:if Arithmetic [allprod_index] && [allprod_index] % [cfg "product table" cols 3] == 0:>
    </tr>
    <tr><:or Arithmetic:><:eif Arithmetic:>
      <td><:if Admin:>
        <div class="arrows"><:include admin/adminmenu/allprod_mini.tmpl:><:moveallprod:></div><:or Admin:><:eif Admin:>
        <a href="<:url allprod:>" class="thumbnail"><:switch:><:case Allprod thumbImage:><:thumbnail allprod:><:case default:><:sthumbimage allprod product_table thumbnail,1:><:endswitch:></a>
        <a href="<:url allprod:>" class="title"><:allprod title:></a><:if And [allprod description] [cfg shop product_description 1]:>
        <p class="summary"><:allprod description:></p><:or And:><:eif And:>
        <div class="details">
          <p class="price"><:ifAllprod retailPrice:><:cfg shop currency "$":><:money allprod retailPrice:><:or:>NA<:eif:></p><:ifAllprod retailPrice:>
          <p class="options"><input type="hidden" id="qty<:allprod id:>" name="qty<:allprod id:>" value="" /><input type="image" name="a_addsingle<:allprod id:>" id="a_addsingle<:allprod id:>" src="/images/basic/store/buttons/add.gif" title="Add this item" class="shop_addsingle" /></p><:or:><:eif:>
        </div>
      </td><:iterator end allprods:><:if Arithmetic [allprod_count] % [cfg "product table" cols 3]:><:if Cfg "product table" colspan:>
      <td colspan="<:arithmetic [cfg "product table" cols 3] - ( [allprod_count] % [cfg "product table" cols 3] ):>">&nbsp;</td><:or Cfg:><:iterator begin strepeats [arithmetic [cfg "product table" cols 3] - ( [allprod_count] % [cfg "product table" cols 3] )]:>
      <td>&nbsp;</td><:iterator end strepeats:><:eif Cfg:><:or Arithmetic:><:eif Arithmetic:>
    </tr>
  </table>
</div><:endswitch:><:switch:><:case Allcats:>
<div class="children">
  <table class="catalogs">
    <tr><:iterator begin allcats:><:if Arithmetic [allcat_index] && [allcat_index] % [cfg "catalog table" cols 3] == 0:>
    </tr>
    <tr><:or Arithmetic:><:eif Arithmetic:>
      <td><:if Admin:>
        <div class="arrows"><:include admin/adminmenu/allcat_mini.tmpl:><:moveallcat:></div><:or Admin:><:eif Admin:>
        <a href="<:url allcat:>" class="thumbnail"><:switch:><:case Allcat thumbImage:><:thumbnail allcat:><:case default:><:sthumbimage allcat catalog_table thumbnail,1:><:endswitch:></a>
        <a href="<:url allcat:>" class="title"><:allcat title:></a><:switch:><:case Allcat summary:>
        <p class="summary"><:allcat summary:></p><:case Allcat summaryLength:>
        <p class="summary"><:summary allcat:></p><:endswitch:>
      </td><:iterator end allcats:><:if Arithmetic [allcat_count] % [cfg "catalog table" cols 3]:><:if Cfg "catalog table" colspan:>
      <td colspan="<:arithmetic [cfg "catalog table" cols 3] - ( [allcat_count] % [cfg "catalog table" cols 3] ):>">&nbsp;</td><:or Cfg:><:iterator begin strepeats [arithmetic [cfg "catalog table" cols 3] - ( [allcat_count] % [cfg "catalog table" cols 3] )]:>
      <td>&nbsp;</td><:iterator end strepeats:><:eif Cfg:><:or Arithmetic:><:eif Arithmetic:>
    </tr>
  </table>
</div><:endswitch:>
</form><:if Or [ifEmbedded] [cfg basic hide_stepparent_list]:><:or Or:><:if Stepparents:>
<div class="relatedsections">
  <p><strong>Related sections:</strong></p>
  <ul><:iterator begin stepparents:>
    <li><a href="<:url stepparent:>"><:stepparent title:></a></li><:iterator end stepparents:>
  </ul>
</div><:or Stepparents:><:eif Stepparents:><:eif Or:><:if Files:><:if Or [cfg basic hide_file_list] [ifMatch [article flags] "H"]:><:or Or:>
<:include include/files.tmpl:><:eif Or:><:or Files:><:eif Files:>
</div>
<!-- END ID: <:article id:> - TEMP: <:article template:> -->
<:embed end:>