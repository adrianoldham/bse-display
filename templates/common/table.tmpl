  <table>
    <tr><:iterator begin allkids:><:if Arithmetic [allkid_index] && [allpkid_index] % [cfg "article table" cols 3] == 0:>
    </tr>
    <tr><:or Arithmetic:><:eif Arithmetic:>
      <td class="child">
